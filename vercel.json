{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://localhost:5000/:path*"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://your-backend-url.com"
  }
}
